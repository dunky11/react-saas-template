{
  "name": "react-saas-template",
  "version": "1.0.0",
  "private": true,
  "homepage": "",
  "description": "Template for building an SaaS/admin application using React + Material-UI",
  "dependencies": {
    "@babel/core": "^7.8.4",
    "@date-io/date-fns": "^2.3.0",
    "@material-ui/core": "^4.9.2",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/pickers": "^3.2.10",
    "@material-ui/system": "^4.9.1",
    "@svgr/webpack": "^5.1.0",
    "aos": "^2.3.4",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "^25.1.0",
    "babel-loader": "8.0.6",
    "babel-plugin-named-asset-import": "^0.3.6",
    "babel-polyfill": "^6.26.0",
    "babel-preset-react-app": "^9.1.1",
    "bfj": "7.0.2",
    "case-sensitive-paths-webpack-plugin": "2.3.0",
    "classnames": "^2.2.6",
    "css-loader": "3.4.2",
    "date-fns": "^2.9.0",
    "dotenv": "8.2.0",
    "dotenv-expand": "5.1.0",
    "emoji-mart": "^2.11.2",
    "file-loader": "5.0.2",
    "fork-ts-checker-webpack-plugin-alt": "0.4.14",
    "fs-extra": "8.1.0",
    "html-webpack-plugin": "4.0.0-beta.11",
    "identity-obj-proxy": "3.0.0",
    "jest": "^25.1.0",
    "jest-pnp-resolver": "1.2.1",
    "jest-resolve": "25.1.0",
    "jest-watch-typeahead": "^0.4.2",
    "js-cookie": "^2.2.0",
    "mini-css-extract-plugin": "0.9.0",
    "optimize-css-assets-webpack-plugin": "5.0.3",
    "pnp-webpack-plugin": "1.6.0",
    "postcss-flexbugs-fixes": "4.2.0",
    "postcss-loader": "3.0.0",
    "postcss-preset-env": "6.7.0",
    "postcss-safe-parser": "4.0.2",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-app-polyfill": "^1.0.6",
    "react-cropper": "^1.3.0",
    "react-dom": "^16.12.0",
    "react-dropzone": "^10.2.1",
    "react-payment-inputs": "^1.1.2",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "recharts": "^1.8.5",
    "sass-loader": "8.0.2",
    "style-loader": "1.1.3",
    "styled-components": "^5.0.1",
    "terser-webpack-plugin": "^2.3.4",
    "url-loader": "3.0.0",
    "webpack": "4.41.6",
    "webpack-dev-server": "3.10.3",
    "webpack-manifest-plugin": "2.2.0",
    "workbox-webpack-plugin": "5.0.0"
  },
  "scripts": {
    "start": "node scripts/start.js",
    "build": "node scripts/build.js",
    "test": "node scripts/test.js --env=jsdom"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "resolver": "jest-pnp-resolver",
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}"
    ],
    "testEnvironment": "jsdom",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "devDependencies": {
    "browserslist": "^4.8.6",
    "caniuse-lite": "^1.0.30001027",
    "react-dev-utils": "^10.2.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dunky11/react-saas-template.git"
  },
  "author": "dunky11",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dunky11/react-saas-template/issues"
  }
}
